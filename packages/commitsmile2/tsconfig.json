{
	"$schema": "http://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"jsx": "react",
		"outDir": "./dist",
		"baseUrl": "src",
		"paths": {
			"@/*": ["*", "*/index.ts", "*.ts"]
		},
		"stripInternal": true,
		"plugins": [
			{
				"transform": "typia/lib/transform"
			}
		],
		"preserveSymlinks": true,
		"strict": true
	},
	"extends": "@ineedj/tsconfig/base.json"
}
